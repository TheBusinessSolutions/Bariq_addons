<odoo>
    <record id="view_picking_tree_inherit_dawar_ticket" model="ir.ui.view">
        <field name="name">stock.picking.tree.inherit.dawar.ticket</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.vpicktree"/>
        <field name="arch" type="xml">
            <!-- Target the origin field and place dawar_ticket right after it -->
            <field name="origin" position="after">
                <field name="dawar_ticket"/>
            </field>
        </field>
    </record>
</odoo>
