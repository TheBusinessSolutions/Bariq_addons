<odoo>
    <data>
        <!-- Inherit the stock.picking list view to include dawar_ticket field -->
        <record id="view_stock_picking_list_inherit" model="ir.ui.view">
            <field name="name">stock.picking.list.inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_tree"/> <!-- Use the correct external ID -->
            <field name="arch" type="xml">
                <xpath expr="//field[@name='origin']" position="after">
                    <!-- Add the dawar_ticket field after the origin field -->
                    <field name="dawar_ticket"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
